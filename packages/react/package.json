{
  "name": "@wizard/react",
  "version": "0.0.1",
  "description": "React adapter for @wizard/core - hooks and provider for type-safe multi-step wizards",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@wizard/core": "workspace:*"
  },
  "peerDependencies": {
    "@tanstack/react-store": "^0.7.0",
    "@tanstack/react-router": "^1.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-router": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tanstack/react-store": "^0.7.5",
    "@tanstack/react-router": "^1.131.44",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.13",
    "react": "^19.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/user/wizard"
  },
  "keywords": [
    "wizard",
    "react",
    "multi-step",
    "form",
    "stepper",
    "typescript",
    "headless"
  ],
  "author": "",
  "license": "MIT"
}